{
  "multipart": [
    {
      "apply": {
        "model": "minecraft:block/brewing_stand"
      }
    },
    {
      "apply": {
        "model": "minecraft:block/brewing_stand_bottle0"
      },
      "when": {
        "has_bottle_0": "true"
      }
    },
    {
      "apply": {
        "model": "minecraft:block/brewing_stand_bottle1"
      },
      "when": {
        "has_bottle_1": "true"
      }
    },
    {
      "apply": {
        "model": "minecraft:block/brewing_stand_bottle2"
      },
      "when": {
        "has_bottle_2": "true"
      }
    },
    {
      "apply": {
        "model": "minecraft:block/brewing_stand_empty0"
      },
      "when": {
        "has_bottle_0": "false"
      }
    },
    {
      "apply": {
        "model": "minecraft:block/brewing_stand_empty1"
      },
      "when": {
        "has_bottle_1": "false"
      }
    },
    {
      "apply": {
        "model": "minecraft:block/brewing_stand_empty2"
      },
      "when": {
        "has_bottle_2": "false"
      }
    },
    {
      "apply": [
        {
          "model": "grid_visible_block_edges:block/main",
          "uvlock": false,
          "x": 0
        },
        {
          "model": "grid_visible_block_edges:block/main",
          "uvlock": false,
          "x": 90
        },
        {
          "model": "grid_visible_block_edges:block/main",
          "uvlock": false,
          "x": 180
        },
        {
          "model": "grid_visible_block_edges:block/main",
          "uvlock": false,
          "x": 270
        }
      ],
      "when": {
        "AND": []
      }
    }
  ]
}