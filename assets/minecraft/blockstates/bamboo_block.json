{
  "multipart": [
    {
      "apply": {
        "model": "minecraft:block/bamboo_block_x"
      },
      "when": {
        "AND": [
          {
            "axis": "x"
          }
        ]
      }
    },
    {
      "apply": {
        "model": "minecraft:block/bamboo_block_y"
      },
      "when": {
        "AND": [
          {
            "axis": "y"
          }
        ]
      }
    },
    {
      "apply": {
        "model": "minecraft:block/bamboo_block_z"
      },
      "when": {
        "AND": [
          {
            "axis": "z"
          }
        ]
      }
    },
    {
      "apply": [
        {
          "model": "grid_visible_block_edges:block/main",
          "uvlock": false,
          "x": 0
        },
        {
          "model": "grid_visible_block_edges:block/main",
          "uvlock": false,
          "x": 90
        },
        {
          "model": "grid_visible_block_edges:block/main",
          "uvlock": false,
          "x": 180
        },
        {
          "model": "grid_visible_block_edges:block/main",
          "uvlock": false,
          "x": 270
        }
      ],
      "when": {
        "AND": []
      }
    }
  ]
}