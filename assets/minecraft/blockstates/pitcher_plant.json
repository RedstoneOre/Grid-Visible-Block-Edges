{
  "multipart": [
    {
      "apply": {
        "model": "minecraft:block/pitcher_plant_bottom"
      },
      "when": {
        "AND": [
          {
            "half": "lower"
          }
        ]
      }
    },
    {
      "apply": {
        "model": "minecraft:block/pitcher_plant_top"
      },
      "when": {
        "AND": [
          {
            "half": "upper"
          }
        ]
      }
    },
    {
      "apply": [
        {
          "model": "grid_visible_block_edges:block/main",
          "uvlock": false,
          "x": 0
        },
        {
          "model": "grid_visible_block_edges:block/main",
          "uvlock": false,
          "x": 90
        },
        {
          "model": "grid_visible_block_edges:block/main",
          "uvlock": false,
          "x": 180
        },
        {
          "model": "grid_visible_block_edges:block/main",
          "uvlock": false,
          "x": 270
        }
      ],
      "when": {
        "AND": []
      }
    }
  ]
}